{
  "name": "几枝",
  "version": "1.0",
  "description": "Build an Extension!",
  "permissions": ["storage", "tabs"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "chrome_url_overrides" : {
    "newtab": "index.html"
  },
  "icons": {
    "16": "icons/jizhi-16.png",
    "32": "icons/jizhi-32.png",
    "48": "icons/jizhi-64.png",
    "128":"icons/jizhi-128.png"
  },
  "manifest_version": 2
}
